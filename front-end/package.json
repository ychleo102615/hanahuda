{
  "name": "front-end",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "dev:test": "DATABASE_URL=postgresql://test:test@localhost:5433/hanafuda_test nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:client": "vitest run tests/client",
    "test:unit:server": "vitest run tests/server",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "type-check": "nuxt typecheck",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:test:up": "docker compose -f docker-compose.test.yml up -d",
    "db:test:down": "docker compose -f docker-compose.test.yml down",
    "db:test:migrate": "DATABASE_URL=postgresql://test:test@localhost:5433/hanafuda_test drizzle-kit migrate",
    "db:test:studio": "DATABASE_URL=postgresql://test:test@localhost:5433/hanafuda_test drizzle-kit studio",
    "db:test:setup": "pnpm db:test:up && sleep 3 && pnpm db:test:migrate",
    "expose": "./scripts/expose-dev.sh",
    "ci": "./scripts/ci-check.sh"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/motion": "^3.0.3",
    "@vueuse/nuxt": "^14.1.0",
    "arctic": "^3.7.0",
    "bcryptjs": "^3.0.3",
    "crossws": "^0.3.5",
    "drizzle-orm": "^0.45.0",
    "nuxt": "^4.2.1",
    "pinia": "^3.0.4",
    "postgres": "^3.4.7",
    "uuid": "^13.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.22",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@types/pg": "^8.15.6",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.37.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.0",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1",
    "ws": "^8.19.0"
  }
}
